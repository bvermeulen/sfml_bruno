{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug move_weight",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/debug/move_weight_dbg.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "DEBUG move_weight",
            "miDebuggerPath": "d:/cpp/msys64/mingw64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
